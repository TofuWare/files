-- Scripts:

local function RBKW_fake_script() -- whatsthenameo.Gaybow script 
	local script = Instance.new('LocalScript', whatsthenameo)

	local text = script.Parent
	
	while wait() do
		text.TextColor3 = Color3.fromRGB(255)
		wait()
		text.TextColor3 = Color3.fromRGB(255,128)
		wait()
		text.TextColor3 = Color3.fromRGB(255,255)
		wait()
		text.TextColor3 = Color3.fromRGB(128,255)
		wait()
		text.TextColor3 = Color3.fromRGB(0,255)
		wait()
		text.TextColor3 = Color3.fromRGB(0,255,128)
		wait()
		text.TextColor3 = Color3.fromRGB(0,255,255)
		wait()
		text.TextColor3 = Color3.fromRGB(0,128,255)
		wait()
		text.TextColor3 = Color3.fromRGB(0,0,255)
		wait()
		text.TextColor3 = Color3.fromRGB(128,0,255)
		wait()
		text.TextColor3 = Color3.fromRGB(255,0,255)
		wait()
		text.TextColor3 = Color3.fromRGB(255,0,128)
		wait()
	end
end
coroutine.wrap(RBKW_fake_script)()
local function WIUQOU_fake_script() -- Closethegui.The close script 
	local script = Instance.new('LocalScript', Closethegui)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(WIUQOU_fake_script)()
local function QJGBE_fake_script() -- Stuff2.Drag script <.< 
	local script = Instance.new('LocalScript', Stuff2)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(QJGBE_fake_script)()
